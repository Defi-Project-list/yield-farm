{
  "name": "hon-yield-farm",
  "version": "1.0.0",
  "description": "Heroes of NFT - HON yield farm is a time based yield farm where to collect HON tokens",
  "main": "hardhat.config.js",
  "directories": {
    "test": "test",
    "deploy": "deploy"
  },
  "scripts": {
    "build": "npx hardhat compile",
    "console": "npx hardhat console",
    "test": "npx hardhat test --verbose",
    "lint": "yarn prettier && solhint -c .solhint.json contracts/**/*.sol",
    "avash:deploy": "npx hardhat --network avash deploy --export exports/avash/farm-exports.json",
    "fuji:deploy": "npx hardhat --network fuji deploy --export exports/fuji/farm-exports.json",
    "fuji:export": "npx hardhat --network fuji export --export exports/fuji/farm-exports.json",
    "mainnet:deploy": "npx hardhat --network mainnet deploy --export exports/mainnet/farm-exports.json"
  },
  "keywords": [
    "heroesofnft",
    "heroes",
    "nft",
    "avalanche",
    "avax"
  ],
  "author": "defikintaro",
  "license": "GPL-2.0 or Later",
  "devDependencies": {
    "@ethersproject/bignumber": "^5.1.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.0.0",
    "@openzeppelin/test-helpers": "^0.5.10",
    "@typechain/ethers-v5": "^6.0.5",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "avalanche": "^3.8.5",
    "chai": "^4.3.4",
    "ethers": "^5.1.0",
    "hardhat": "^2.2.0",
    "hardhat-abi-exporter": "^2.2.0",
    "hardhat-deploy": "^0.7.4",
    "hardhat-deploy-ethers": "^0.3.0-beta.7",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-typechain": "^0.3.5",
    "hardhat-watcher": "^2.1.1",
    "prettier": "^2.3.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.3.5",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "@pangolindex/exchange-contracts": "^1.0.1"
  }
}
